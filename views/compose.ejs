<%- include("partials/header") -%>

<h1>Compose</h1>

<form class="item" action="/compose" method="post">
  <div class="form-group">
  <label>Title</label>
  <input type="text" name="postTitle" class="form-control">
  </div>
  <div class="form-group">
  <label>Post</label>
  <textarea type="text" name="postBody" class="form-control" rows="3"></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>





<%- include("partials/footer") -%>
